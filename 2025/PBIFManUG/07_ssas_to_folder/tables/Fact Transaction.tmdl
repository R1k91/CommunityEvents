table 'Fact Transaction'
	ordinal: 14

	partition Partition = M
		dataView: Full
		expression:=
			let
			    Source = #"SQL/localhost;WideWorldImportersDW",
			    Fact_Transaction = Source{[Schema="Fact",Item="Transaction"]}[Data]
			in
			    Fact_Transaction

	column 'Transaction Key'
